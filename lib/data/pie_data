import 'dart:ui';

class PieData{
  static List<Data> data = [
   Data(name:'Blue',percent:50,color:  Color(0xFF8B4CFC)),
  Data(name:'Red',percent:25,color: const Color(0xFFC2A3FC)),
  Data(name:'Green',percent:18,color: const Color(0xFFDFD2FE)),
  Data(name:'Dark Yellow',percent:10,color:  Color(0xFFF4EEFF)),

  ];
}

class Data {
  final String name;
  final double percent;
  Color color;

  Data({required this.name, required
  this.percent, required this.color
  });
}